{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://algorank-backend.onrender.com",
    "VITE_EXECUTOR_URL": "https://algorank-executor.onrender.com",
    "VITE_ENV": "production"
  },
  "github": {
    "silent": true,
    "autoAlias": true
  }
} 